{
  "Comment": "A Hello World example of the Amazon States Language",
  "StartAt": "Run-AWSLambdaFunction1",
  "States": {
    "Run-AWSLambdaFunction2": {
      "Type": "Task",
      "Resource": "${AWSLambdaFunction2}",
      "ResultPath": "$.result",
      "Next": "Success-State"
    },
    "Fail-State": {
      "Type": "Fail",
      "Error": "$.result",
      "Cause": "Caused By Message"
    },
    "Success-State": {
      "Type": "Succeed"
    }
  }
}
